<resources xmlns:tools="http://schemas.android.com/tools">

  <!-- Accent colors -->
  <eat-comment/>
  <color name="cyanea_accent_reference">#FFF29135</color>
  <color name="cyanea_accent_light_reference">#FFFDA34D</color>
  <color name="cyanea_accent_dark_reference">#FFE68529</color>

  <!-- Primary colors -->
  <eat-comment/>
  <color name="cyanea_primary_reference">#FF65185A</color>
  <color name="cyanea_primary_light_reference">#FF8c2D7F</color>
  <color name="cyanea_primary_dark_reference">#FF57114D</color>

  <!-- Background colors -->
  <eat-comment/>
  <color name="cyanea_background_dark">#FF303030</color>
  <color name="cyanea_background_dark_darker">#FF1E1E1E</color>
  <color name="cyanea_background_dark_lighter">#FF2D2D2D</color>
  <color name="cyanea_background_light">#FFFAFAFA</color>
  <color name="cyanea_background_light_darker">#FFC0C0C0</color>
  <color name="cyanea_background_light_lighter">#FFF8F8F8</color>
  <color name="background_material_dark" tools:override="true">@color/cyanea_background_dark</color>
  <color name="background_material_light" tools:override="true">@color/cyanea_background_light</color>

  <!-- Navigation bar color -->
  <eat-comment/>
  <color name="cyanea_navigation_bar_reference">@color/cyanea_primary_reference</color>

  <!-- Menu colors -->
  <eat-comment/>
  <color name="cyanea_menu_icon_light">#FFFFFFFF</color>
  <color name="cyanea_menu_icon_dark">#DE000000</color>
  <color name="cyanea_sub_menu_icon_light">#B3FFFFFF</color>
  <color name="cyanea_sub_menu_icon_dark">#8A000000</color>

  <!-- Material colors -->
  <eat-comment/>
  <color name="cyanea_selector_default">#FF9E9E9E</color>

  <!-- Background selector colors -->
  <eat-comment/>
  <color name="cyanea_disabled_selected_reference">#FF757575</color>

  <!-- Translucent window background theme color -->
  <eat-comment/>
  <color name="cyanea_translucent_background">#99323232</color>

  <!-- Checkable ColorStateList -->
  <eat-comment/>
  <color name="cyanea_switch_track_disabled_dark">#1AFFFFFF</color>
  <color name="cyanea_switch_track_disabled_light">#1F000000</color>
  <color name="cyanea_switch_track_normal_dark">#4DFFFFFF</color>
  <color name="cyanea_switch_track_normal_light">#43000000</color>

</resources>